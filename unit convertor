a = float(input("Enter the time in hours:"))
b = float(a*60)
print("your time in minutes are",b)
d = float(input("Enter the distance in mile here:"))
e = float(d*1.6)
print("your distance in kilometere is",e)
f = float(input("Enter the weight in kilogram hare"))
g = float(f*2.2)
print("your wieght in pound is",g)
c = float(input("enter the temprature in celcius:"))
f = float((c * 1.8)+32 )
print("your temprature in f is",f)
